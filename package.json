{
  "name": "@pristine-ts/class-validator",
  "version": "2.0.0",
  "description": "",
  "module": "dist/lib/esm/class-validator.js",
  "main": "dist/lib/cjs/class-validator.js",
  "types": "dist/types/class-validator.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "prepare": "npm run build",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magieno/pristine-validator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/magieno/pristine-validator/issues"
  },
  "homepage": "https://github.com/magieno/pristine-validator#readme",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/validator": "^13.7.3",
    "class-transformer": "^0.5.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "@pristine-ts/metadata": "^1.0.13",
    "date-fns": "^3.6.0",
    "libphonenumber-js": "^1.11.3",
    "reflect-metadata": "^0.2.2",
    "validator": "^13.12.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": {
          "strictNullChecks": false
        }
      }
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  }
}
