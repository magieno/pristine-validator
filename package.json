{
  "name": "@pristine/class-validator",
  "version": "1.0.0",
  "description": "",
  "main": "src/validator.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "prepare": "npm run build",
    "prepublishOnly": "git push --follow-tags",
    "test": "jest",
    "test:cov": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magieno/pristine-validator.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/magieno/pristine-validator/issues"
  },
  "homepage": "https://github.com/magieno/pristine-validator#readme",
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/validator": "^13.7.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "validator": "^13.7.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": {
          "strictNullChecks": false
        }
      }
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,ts}"
    ]
  }
}
